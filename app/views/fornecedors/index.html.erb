<h1>Listing fornecedors</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Morada</th>
    <th>Localidade</th>
    <th>Numerocontribuinte</th>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fornecedors.each do |fornecedor| %>
  <tr>
    <td><%= fornecedor.nome %></td>
    <td><%= fornecedor.morada %></td>
    <td><%= fornecedor.localidade %></td>
    <td><%= fornecedor.numeroContribuinte %></td>
    <td><%= fornecedor.descricao %></td>
    <td><%= link_to 'Show', fornecedor %></td>
    <td><%= link_to 'Edit', edit_fornecedor_path(fornecedor) %></td>
    <td><%= link_to 'Destroy', fornecedor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fornecedor', new_fornecedor_path %>
